			<test logic="AND" value="fbw/pitch/e-f-wow">
				fbw/fmgc/enabled eq 0
				/position/gear-agl-ft le 30
				/controls/flight/flaps-pos ge 4
				/controls/engines/engine[0]/throttle lt 0.65
				/controls/engines/engine[1]/throttle lt 0.65
