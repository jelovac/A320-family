			<kp>fbw/pitch/p-gain</kp>
			<ki>fbw/pitch/i-gain</ki>
			<kd>fbw/pitch/d-gain</kd>
			<trigger>fbw/pitch/trigger</trigger>
			<clipto>
				<min>-1.0</min>
				<max>1.0</max>
