					</table>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<independentVar lookup="column">position/wow</independentVar>
						<tableData>
							     0    1
							140  0.5  1.0
							350  0.1  1.0
						</tableData>
					</table>
				</product>
			</function>
		</fcs_function>
		
		<switch name="fbw/roll/s-i">
